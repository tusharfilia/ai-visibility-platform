{
  "name": "@ai-visibility/api",
  "version": "1.0.0",
  "description": "NestJS REST API for AI Visibility Platform",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "cd /app && NODE_PATH=/app/node_modules node apps/api/dist/main.js",
    "start:prod": "cd /app && NODE_PATH=/app/node_modules node apps/api/dist/main.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:batch1": "tsx src/test-batch1.ts",
    "start:test": "ts-node -r tsconfig-paths/register src/main-test.ts"
  },
  "dependencies": {
    "@ai-visibility/automation": "workspace:*",
    "@ai-visibility/content": "workspace:*",
    "@ai-visibility/copilot": "workspace:*",
    "@ai-visibility/db": "workspace:*",
    "@ai-visibility/geo": "workspace:*",
    "@ai-visibility/prompts": "workspace:*",
    "@ai-visibility/shared": "workspace:*",
    "@anthropic-ai/sdk": "^0.9.1",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "@azure/openai": "^2.0.0",
    "@google/generative-ai": "^0.2.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
