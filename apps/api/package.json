    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.0.1",
    "axios": "^1.6.2",
    "bullmq": "^4.15.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "docx": "^8.5.0",
    "events": "^3.3.0",
    "express-rate-limit": "^7.1.5",
