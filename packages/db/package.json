{
  "name": "@ai-visibility/db",
  "version": "1.0.0",
  "description": "Database schema and utilities for AI Visibility Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "migrate": "prisma migrate dev",
    "push": "prisma db push",
    "reset": "prisma migrate reset",
    "seed": "tsx prisma/seed.ts",
    "generate": "prisma generate",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@ai-visibility/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "prisma": "^5.7.0"
  }
}
